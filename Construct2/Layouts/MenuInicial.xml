<?xml version="1.0" encoding="utf-8" ?>
<c2layout>
    <name>MenuInicial</name>
    <event-sheet>MenuInicial</event-sheet>
    <size>
        <width>800</width>
        <height>600</height>
    </size>
    <margins>
        <horizontal>500</horizontal>
        <vertical>500</vertical>
    </margins>
    <unbounded-scrolling>0</unbounded-scrolling>
    <layers>
        <layer name="Layer 0">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="imgBackground">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <effect>(none)</effect>
                        <hotspot>Top-left</hotspot>
                    </properties>
                    <world>
                        <x>-117</x>
                        <y>-84</y>
                        <z>0</z>
                        <width>1068</width>
                        <height>782</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
                <instance type="txtOracao">
                    <properties>
                        <text>Os Articuladores textuais!</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Brush Script Std,-29,500,0</font>
                        <color>255,140,140</color>
                        <horizontal-alignment>Left</horizontal-alignment>
                        <hotspot>Top-left</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <behavior-instances>
                        <behavior-instance type="Pin" />
                    </behavior-instances>
                    <world>
                        <x>195</x>
                        <y>104</y>
                        <z>0</z>
                        <width>303</width>
                        <height>99</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
        <layer name="layProf">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>1</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="imgProfessor">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <initial-frame>0</initial-frame>
                        <effect>(none)</effect>
                    </properties>
                    <behavior-instances>
                        <behavior-instance type="Pin" />
                    </behavior-instances>
                    <world>
                        <x>-168</x>
                        <y>591</y>
                        <z>0</z>
                        <width>279.412536621</width>
                        <height>634.781860352</height>
                        <depth>0</depth>
                        <hotspotX>-0.004854369</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0.52359879</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
                <instance type="Botao">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <initial-frame>0</initial-frame>
                        <effect>(none)</effect>
                    </properties>
                    <world>
                        <x>664</x>
                        <y>560</y>
                        <z>0</z>
                        <width>270</width>
                        <height>80</height>
                        <depth>0</depth>
                        <hotspotX>0.5</hotspotX>
                        <hotspotY>0.5</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
        <layer name="layFade">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances />
        </layer>
    </layers>
    <nonworld-instances />
</c2layout>
