<?xml version="1.0" encoding="utf-8" ?>
<c2layout>
    <name>Finalizacao</name>
    <event-sheet>Finalizacao</event-sheet>
    <size>
        <width>800</width>
        <height>600</height>
    </size>
    <margins>
        <horizontal>500</horizontal>
        <vertical>500</vertical>
    </margins>
    <unbounded-scrolling>0</unbounded-scrolling>
    <layers>
        <layer name="layBg">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="imgBackground">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <effect>(none)</effect>
                        <hotspot>Top-left</hotspot>
                    </properties>
                    <world>
                        <x>-117</x>
                        <y>-84</y>
                        <z>0</z>
                        <width>1068</width>
                        <height>782</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
        <layer name="layProf">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
