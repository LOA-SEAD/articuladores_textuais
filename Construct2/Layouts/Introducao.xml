<?xml version="1.0" encoding="utf-8" ?>
<c2layout>
    <name>Introducao</name>
    <event-sheet>Introducao</event-sheet>
    <size>
        <width>800</width>
        <height>600</height>
    </size>
    <margins>
        <horizontal>500</horizontal>
        <vertical>500</vertical>
    </margins>
    <unbounded-scrolling>0</unbounded-scrolling>
    <layers>
        <layer name="Layer 0">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="txtApresentam">
                    <properties>
                        <text>apresentam</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Calibri,-27,400,0</font>
                        <color>68,68,68</color>
                        <horizontal-alignment>Center</horizontal-alignment>
                        <hotspot>Center</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <instance-variables>
                        <FadeFlag>0</FadeFlag>
                    </instance-variables>
                    <world>
                        <x>400</x>
                        <y>300</y>
                        <z>0</z>
                        <width>200</width>
                        <height>50</height>
                        <depth>0</depth>
                        <hotspotX>0.5</hotspotX>
                        <hotspotY>0.5</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
    </layers>
    <nonworld-instances />
</c2layout>
